{
  "name": "batsi-ng-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "pretty:batsi-ng": "prettier --write ./projects/batsi-ng/src/app/**/*.{ts,js,css,scss,json}",
    "lint": "ng lint",
    "lint:style:batsi-ng": "stylelint \"projects/batsi-ng/src/**/*.{css,scss}\"",
    "lint:staged": "lint-staged",
    "prepare": "cd .. && husky install ./batsi-ng-workspace/.husky",
    "build:models": "yarn pretty:definitions && yarn generate:models && yarn pretty:models && ng build --project=batsi-models",
    "pretty:definitions": "prettier --write ./projects/batsi-models/src/lib/definitions/*.yaml",
    "generate:models": "openapi-generator-cli generate -g typescript-angular -i projects/batsi-models/src/lib/definitions/swagger.yaml -o projects/batsi-models/src/lib/typescript-client --global-property models,supportingFiles --additional-properties=stringEnums=true,npmName=batsi-models --skip-validate-spec",
    "pretty:models": "prettier --write ./projects/batsi-models/src/lib/typescript-client/**/*.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.1.1",
    "@angular/common": "15.1.1",
    "@angular/compiler": "15.1.1",
    "@angular/core": "15.1.1",
    "@angular/forms": "15.1.1",
    "@angular/platform-browser": "15.1.1",
    "@angular/platform-browser-dynamic": "15.1.1",
    "@angular/router": "15.1.1",
    "bootstrap": "5.2.3",
    "rxjs": "7.8.0",
    "tslib": "2.4.1",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "15.0.0",
    "@angular-devkit/build-angular": "15.1.2",
    "@angular-eslint/builder": "15.2.0",
    "@angular-eslint/eslint-plugin": "15.2.0",
    "@angular-eslint/eslint-plugin-template": "15.2.0",
    "@angular-eslint/schematics": "15.2.0",
    "@angular-eslint/template-parser": "15.2.0",
    "@angular/cli": "15.1.2",
    "@angular/compiler-cli": "15.1.1",
    "@briebug/jest-schematic": "6.0.0",
    "@openapitools/openapi-generator-cli": "2.4.26",
    "@types/jest": "29.2.6",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "eslint": "8.32.0",
    "husky": "8.0.3",
    "jest": "28.1.3",
    "lint-staged": "13.1.0",
    "ng-packagr": "^15.1.0",
    "postcss": "8.4.21",
    "postcss-scss": "4.0.6",
    "prettier": "2.8.3",
    "sass": "1.57.1",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "29.0.0",
    "stylelint-scss": "4.3.0",
    "typescript": "4.9.4"
  }
}
